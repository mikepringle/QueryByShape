<Project>
	<PropertyGroup>
		<Nullable>enable</Nullable>
		<ImplicitUsings>false</ImplicitUsings>
		<LangVersion>Latest</LangVersion>
	</PropertyGroup>

	<ItemGroup>
		<InternalsVisibleTo Include="QueryByShape.Analyzer.Tests" />
	</ItemGroup>
	
	<PropertyGroup>
		<Authors>mikepringle</Authors>
		<Company>Mike Pringle</Company>
		<Copyright>Copyright Â© Mike Pringle</Copyright>
		<PackageVersion>$(Version)</PackageVersion>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/mikepringle/QueryByShape</PackageProjectUrl>
		<PackageIcon>QueryByShape.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/mikepringle/QueryByShape</RepositoryUrl>
		<PackageTags>GraphQL client query generate source generator</PackageTags>
		<NoPackageAnalysis>true</NoPackageAnalysis>
		<GeneratePackageOnBuild>False</GeneratePackageOnBuild>
	</PropertyGroup>
</Project>



